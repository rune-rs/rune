const MAX_NUMBER_TO_CHECK = 10_000;

let prime_mask = [];

prime_mask.resize(MAX_NUMBER_TO_CHECK, true);

prime_mask[0] = false;
prime_mask[1] = false;

let total_primes_found = 0;

for p in 2..MAX_NUMBER_TO_CHECK {
    if prime_mask[p] {
        total_primes_found += 1;
        let i = 2 * p;

        while i < MAX_NUMBER_TO_CHECK {
            prime_mask[i] = false;
            i += p;
        }
    }
}

dbg!(total_primes_found);
