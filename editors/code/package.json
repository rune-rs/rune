{
    "name": "rune-vscode",
    "displayName": "rune-vscode",
    "description": "Language support for Rune",
    "preview": true,
    "private": true,
    "icon": "icon.png",
    "version": "0.6.16",
    "releaseTag": null,
    "publisher": "udoprog",
    "repository": {
        "url": "https://github.com/rune-rs/rune.git",
        "type": "git"
    },
    "homepage": "https://rune-rs.github.io/",
    "license": "MIT OR Apache-2.0",
    "keywords": [
        "rune"
    ],
    "categories": [
        "Programming Languages"
    ],
    "engines": {
        "vscode": "^1.47.1"
    },
    "scripts": {
        "vscode:prepublish": "tsc && rollup -c",
        "package": "vsce package -o rune-vscode.vsix",
        "build": "tsc",
        "watch": "tsc --watch"
    },
    "dependencies": {
        "vscode-languageclient": "7.0.0-next.9",
        "node-fetch": "2.6.1"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^13.0.2",
        "@rollup/plugin-node-resolve": "^8.4.0",
        "@types/vscode": "^1.47.1",
        "@types/node-fetch": "^2.5.7",
        "rollup": "^2.26.9",
        "typescript": "^3.9.7",
        "vsce": "^1.79.5"
    },
    "contributes": {
        "languages": [
            {
                "id": "rune",
                "aliases": [
                    "Rune",
                    "rune"
                ],
                "extensions": [
                    ".rn"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "rune",
                "scopeName": "source.rune",
                "path": "./syntaxes/rune.tmGrammar.json"
            }
        ]
    },
    "activationEvents": [
        "onLanguage:rune"
    ],
    "main": "./out/src/main"
}
